import random

def coin_toss():
    return random.choice(['Heads', 'Tails'])

# Get input from the user
num_tosses = int(input("Enter the number of coin tosses: "))

# Simulate coin tosses and count the outcomes
heads_count = 0
tails_count = 0

for _ in range(num_tosses):
    result = coin_toss()
    if result == 'Heads':
        heads_count += 1
    else:
        tails_count += 1

# Print the results
print("Heads:", heads_count, "Tails:", tails_count)



