#Understanding Local and Global


x = 50

def func(x):
    print(f'X is {x}')
    #Local Reassignment!
    x = 200
    print(f'I JUST LOCALLY CHANGED X TO {x}')

&&
x = 50

def func():
    global x
    print(f'X is {x}')
    
    #Local Reassignment On a global variable!
    x = 'NEW VALUE'
    print(f'I JUST LOCALLY CHANGED global X TO {x}')

    

